{
  "name": "contours-to-classification-client",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm install",
    "use:static:config": "cp ./config/static.config.js ./svelte.config.js",
    "use:node:config": "cp ./config/node.config.js ./svelte.config.js",
    "dev:frontend": "npm run use:static:config && svelte-kit dev",
    "dev": "npm run use:node:config && svelte-kit dev",
    "copy:package:config": "cp ./package.json ../build/client/package.json && cp ./package-lock.json ../build/client/package-lock.json",
    "build:production:dependencies": "cd ../build/client/ && npm ci --production",
    "build:dependencies": "npm run copy:package:config && npm run build:production:dependencies",
    "build": "npm run use:node:config && svelte-kit build && npm run build:dependencies",
    "cleanup": "rm ./svelte.config.js && rm -rf ../build/client/"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.62",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "next",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "p5-svelte": "^3.0.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.14",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "socket.io-client": "^4.4.1"
  },
  "type": "module",
  "module": "es2020"
}