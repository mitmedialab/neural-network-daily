{
  "watch": [
    "./src/server",
    "./src/client/src/"
  ],
  "ext": "ts,js,svelte,json",
  "ignore": [
    "./build/**",
    "./src/client/node_modules",
    "./src/client/.svelte-kit",
    "./src/server/node_modules"
  ],
  "exec": "npm run build && NODE_ENV=dev node ./build/server/server.js"
}