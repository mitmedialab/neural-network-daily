{
  "name": "contours-to-classification-client",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm install",
    "dev": "./node_modules/.bin/nodemon & svelte-kit dev --port 5000",
    "copy:package:config": "cp ./package.json ../build/client/package.json && cp ./package-lock.json ../build/client/package-lock.json",
    "install:production:dependencies": "mkdir ../build/client && cd ../build/client/ && rm -rf ./node_modules/ && npm ci --production",
    "build:dependencies": "npm run copy:package:config && npm run install:production:dependencies",
    "build:svelte": "svelte-kit build && touch ../build/ClientBuilt",
    "build": "npm run build:dependencies && npm run build:svelte",
    "cleanup": "rm -rf ../build/client/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/svelte": "^6.4.19",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.62",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "next",
    "@types/jest": "^27.4.0",
    "babel-loader": "^8.2.4",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "p5-svelte": "^3.0.1",
    "svelte": "^3.46.4",
    "svelte-check": "^2.2.6",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.14",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "socket.io-client": "^4.4.1"
  },
  "type": "module",
  "module": "es2020"
}
