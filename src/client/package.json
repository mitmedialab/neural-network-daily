{
  "name": "contours-to-classification-client",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm install",
    "copy:package:config": "cp package.json ../../build/client/package.json && cp package-lock.json ../../build/client/package-lock.json",
    "build:production:dependencies": "cd ../../build/client/ && npm ci --production",
    "build:dependencies": "npm run copy:package:config && npm run build:production:dependencies",
    "build:svelte": "svelte-kit build && npm run build:dependencies",
    "preview:svelte": "svelte-kit preview",
    "check:svelte": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-node": "^1.0.0-next.62",
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "next",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.14",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "socket.io-client": "^4.4.1"
  },
  "type": "module",
  "module": "es2020"
}
