{
  "name": "contours-to-classification-server",
  "version": "0.0.1",
  "scripts": {
    "setup": "npm install",
    "compile:ts": "./node_modules/.bin/tsc -p ./tsconfig.json",
    "copy:dependencies": "cp -r ./node_modules ../build/server/",
    "node:without:requiring:js:extension": "node --experimental-modules --es-module-specifier-resolution=node",
    "server": "npm run node:without:requiring:js:extension -- ../build/server/src/server.js",
    "dev:server": "NODE_ENV=dev npm run server",
    "production:server": "NODE_ENV=production npm run server",
    "dev": "./node_modules/.bin/nodemon",
    "build": "npm run cleanup && npm run compile:ts && npm run copy:dependencies && touch ../build/ServerBuilt",
    "production": "npm run build && npm run production:server",
    "cleanup": "rm -rf ../build/server/"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "connect-livereload": "^0.6.1",
    "express": "^4.17.2",
    "install": "^0.13.0",
    "jsdoc": "^3.6.7",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "npm": "^8.3.1",
    "prettier": "^2.4.1",
    "socket.io": "^4.4.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "module": "es2020",
  "dependencies": {}
}